{# src/Mcms/UserBundle/Resources/views/Admin/editProfile.html.twig #}
{% extends '::dashboard.html.twig' %}

{% block title %}{{ app.user }} - Edit Profile{% endblock %}
{% block content %}
    <form action="{{ path('admin.updateProfile') }}" method="post" {{ form_enctype(form) }}>
        {{ form_errors(form) }}
        {{ form_widget(form) }}

        <input type="submit" />
    </form>
{% endblock %}