{% extends '::clean.html.twig' %}

{% block body %}
	<h1>Create new product.</h1>

	{{ form_errors(form) }}
	<form action="{{ path('product_create') }}" method="post" {{ form_enctype(form) }}>
    	{{ form_row(form.name) }}
    	{{ form_row(form.price) }}
    	{{ form_rest(form) }} 
    <p>
        <button type="submit">Create</button>
    </p>
</form>

{% endblock %}